package com.bit.day25;

import java.io.File;
import java.io.IOException;

public class ObjectStream {
	
	public static void main(String[] args) {
		//ObjectStream
		String path = "data4.bin";
		File file = new File(path);
		if(!file.exists())
			try {
				file.createNewFile();
			} catch (IOException e) {
				e.printStackTrace();
			}
		
	}
	
}
